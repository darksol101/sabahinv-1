#dashboard-summary, #dashboard-log{ float:left; margin-left:35px; width:45%;}
#dashboard-summary .dashboard-item, #dashboard-log .dashboard-log-item{ border-bottom:#49AB3C 1px solid;border-left:#49AB3C 1px solid; border-right:#49AB3C 1px solid; margin:2px; width:100%; -moz-border-radius:3px;border-radius:3px 3px 3px 3px;}
#dashboard-summary .dashboard-item .dashboard-title,  #dashboard-log .dashboard-log-item .dashboard-log-title{ background:#49AB3C; color:#fff ; padding:6px 4px; font-size:14px;}
