#report-dashboard .balancesheet .settings-desc,#report-dashboard .profitandloss .settings-desc, #report-dashboard .trialbalance .settings-desc, #report-dashboard .ledgerst .settings-desc, #report-dashboard .reconciliation .settings-desc{ padding:65px; background:url(../images/docs-icon.png) no-repeat center;}
#report-dashboard .settings-container .settings-title{ font-weight:bold; padding-bottom:7px;}
ul#main-links-nav li { display:inline; list-style:none; background:none}
ul#main-links-nav li a{-moz-border-radius:3px;border-radius:3px 3px 3px 3px;background:repeat-x scroll 0 0 transparent;color:#FFF;font-size:11px;font-weight:700;text-shadow:-1px -1px 0 #333;text-decoration:none;padding:4px 10px}
table.ledgerst-table tr.tr-balance td{font-weight:bold; border-top:#CCC 1px solid;  background-color:#e7e9ec;}
table.ledger-search tr td{ padding:0!important;}
